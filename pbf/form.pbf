Global Form

Global Button_0, Text_0


Procedure OpenForm(x = 0, y = 0, width = 570, height = 140)
  Form = OpenWindow(#PB_Any, x, y, width, height, "", #PB_Window_SystemMenu)
  Button_0 = ButtonGadget(#PB_Any, 250, 100, 100, 25, "")
  Text_0 = TextGadget(#PB_Any, 10, 10, 550, 80, "text")
EndProcedure

Procedure Form_Events(event)
  Select event
    Case #PB_Event_CloseWindow
      ProcedureReturn #False

    Case #PB_Event_Menu
      Select EventMenu()
      EndSelect

    Case #PB_Event_Gadget
      Select EventGadget()
      EndSelect
  EndSelect
  ProcedureReturn #True
EndProcedure


